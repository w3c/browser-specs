{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "url": {
        "type": "string",
        "format": "uri"
      },
      "name": {
        "type": "string",
        "pattern": "^[\\w\\-]+((?<=\\-\\d+)\\.\\d+)?$"
      },
      "shortname": {
        "type": "string",
        "pattern": "^[\\w\\-]+$"
      },
      "level": {
        "type": "number",
        "minimum": 1
      },
      "levelComposition": {
        "type": "string",
        "enum": ["full", "delta"]
      },
      "currentLevel": {
        "type": "string",
        "pattern": "^[\\w\\-]+((?<=\\-\\d+)\\.\\d+)?$"
      },
      "previousLevel": {
        "type": "string",
        "pattern": "^[\\w\\-]+((?<=\\-\\d+)\\.\\d+)?$"
      },
      "nextLevel": {
        "type": "string",
        "pattern": "^[\\w\\-]+((?<=\\-\\d+)\\.\\d+)?$"
      }
    },
    "required": ["url", "name", "shortname", "currentLevel"],
    "additionalProperties": false
  },
  "minItems": 1
}
