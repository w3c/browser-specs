{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "oneOf": [
      {
        "type": "string",
        "format": "uri"
      },
      {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "format": "uri"
          },
          "shortname": {
            "type": "string",
            "pattern": "^[\\w\\-]+((?<=\\-\\d+)\\.\\d+)?$"
          },
          "familyname": {
            "type": "string",
            "pattern": "^[\\w\\-]+$"
          },
          "level": {
            "type": "number",
            "multipleOf": 0.1,
            "minimum": 1
          }
        },
        "required": ["url"],
        "additionalProperties": false
      }
    ]
  },
  "minItems": 1
}
